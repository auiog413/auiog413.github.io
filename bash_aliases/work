# anysdk 项目相关快捷操作
alias anysdk='cd ~/AnySDKServer'
alias anyplugin='cd ~/PluginsServer'

# anysdk server plugin git 相关快捷操作
alias ap-pull-master='anyplugin;git checkout master;git pull gl_group master;cd -'
alias ap-push-master='anyplugin;git checkout master;git push origin master;cd -'
alias ap-pull-release='anyplugin;git checkout release;git pull gl_group release;cd -'
alias ap-push-release='anyplugin;git checkout release;git push origin release;cd -'
alias ap-push-qudao='anyplugin;git checkout master;git pull gl_group master;git push qudao.info master;cd -'

# anysdk server git 相关快捷操作
alias any-check='anysdk;git remote show anysdk;cd -'
alias any-pull-master='anysdk;git checkout master;git pull anysdk master;cd -'
alias any-push-master='anysdk;git checkout master;git push libo master;cd -'
alias any-pull-release='anysdk;git checkout release;git pull anysdk release;cd -'
alias any-push-release='anysdk;git checkout release;git push libo release;cd -'
alias any-pull-sh-master='anysdk;git checkout sh-master;git pull anysdk sh-master;cd -'
alias any-push-sh-master='anysdk;git checkout sh-master;git push libo sh-master;cd -'

# AnyStore git 相关快捷操作
alias anystore='cd ~/AnyStore'
alias store-pull-master='anystore;git checkout master;git pull gl_group master;cd -'
alias store-push-master='anystore;git checkout master;git push origin master;cd -'
alias store-pull-develop='anystore;git checkout develop;git pull gl_group develop;cd -'
alias store-push-develop='anystore;git checkout develop;git push origin develop;cd -'

# anysdk 企业版 git 相关操作
alias cus='cd ~/AnySDKEE'
alias cus-pull-master-1.1='cus;git checkout master-1.1;git pull anysc master-1.1;cd -'
alias cus-push-master-1.1='cus;git checkout master-1.1;git push origin master-1.1;cd -'

# 快速 ssh 连接阿里云服务器
alias sshany='ssh libo@www.anysdk.com'

# 快速链接 服务器
alias sshqd='cd;pass -c server-ssh/qudao.info;ssh root@dev.qudao.info'

# 快速链接 压力测试服务器
alias sshyc='cd;ssh root@117.121.32.103'

# 快速链接 新正式服务器
alias ssh126='cd;pass -c server-ssh/chukong@www-126;ssh chukong@211.151.20.126 -p 50020'
alias ssh127='cd;pass -c server-ssh/chukong@www-126;ssh chukong@211.151.20.127 -p 50020'

# 清除 dev 模版缓存
alias devclean='cd ~/AnySDKServer/EntManage/system/libraries/templates_c;rm -f *.html.php;cd -'
# 清除 sys 模版缓存
alias sysclean='cd ~/AnySDKServer/UcenterManageV5/system/libraries/templates_c;rm -f *.html.php;cd -'

